{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version" : 2,
    "styles": {
        "spacing": {
            "margin": {
                "bottom": "0px",
                "top": "0px",
                "left": "0px",
                "right": "0px"
            },
            "padding": {
                "bottom": "0px",
                "top": "0px",
                "left": "0px",
                "right": "0px"

            },
            "blockGap": "4rem"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--u-fonr-rubik)",
            "fontSize": "16px",
            "fontStyle": "normal",
            "fontWeight": "normal",
            "lineHeight": "inherit",
            "textDecoration": "none",
            "textTransform": "none"
        },
        "color": {
            "text": "var(--wp--preset--color--u-gray-900)",
            "background": "var(--wp--preset--color--pale-cyan-blue)"
        },
        "blocks": {
            "core/button": {
                "color": {
                    "background": "var(--wp--preset--color--u-warning)"
                    
                }
            },"core/pullquote": {
                "border": {
                    "width": "2px",
                    "style": "solid",
                    "radius": "20px",
                    "color": "var(--wp--preset--color--u-primary)"

                }
            },
            "core/site-title": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--u-fonr-rubik)"
                }
            }
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--u-primary)"
                }                
            }
        }

    },
    "settings": {
        "appearanceTools": true,
        "spacing": {
            "margin": true,
            "padding": true,
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "%"
            ],
            "blockGap": true
        },
        "layout": {
            "contentSize": "800px",
            "wideSize": "1110px"
        },
        "typography": {
            "fontFamilies": [
                {"name": "Udemy Rubik",
            "slug": "u-fonr-rubik",
        "fontFamily": "Rubik"}
            ],
            "fontSizes": [
                {"name": "small","slug": "small","size": "0.75rem"}
            ],
            "dropCap": true,
            "fontWeight": true,
            "textTransform": true,
            "textDecoration": true,
            "lineHeight": true,
            "letterSpacing": true,
            "fontStyle": true
        },
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        },
        "color" : {
            "defaultPalette" : false,
            "palette": [
                { "slug": "u-gray-100", "color": "rgb(243 244 246)", "name": "Udemy Gray 100" },
                { "slug": "u-gray-200", "color": "rgb(229 231 235)", "name": "Udemy Gray 200" },
                { "slug": "u-gray-300", "color": "rgb(209 213 219)", "name": "Udemy Gray 300" },
                { "slug": "u-gray-400", "color": "rgb(156 163 175)", "name": "Udemy Gray 400" },
                { "slug": "u-gray-500", "color": "rgb(107 114 128)", "name": "Udemy Gray 500" },
                { "slug": "u-gray-600", "color": "rgb(75 85 99)", "name": "Udemy Gray 600" },
                { "slug": "u-gray-700", "color": "rgb(55 65 81)", "name": "Udemy Gray 700" },
                { "slug": "u-gray-800", "color": "rgb(31 41 55)", "name": "Udemy Gray 800" },
                { "slug": "u-gray-900", "color": "rgb(17 24 39)", "name": "Udemy Gray 900" },
                { "slug": "u-primary", "color": "rgb(239 68 68)", "name": "Udemy Primary" },
                { "slug": "u-accent", "color": "rgb(67 56 202)", "name": "Udemy Accent" },
                { "slug": "u-success", "color": "rgb(52 211 153)", "name": "Udemy Success" },
                { "slug": "u-info", "color": "rgb(96 165 250)", "name": "Udemy Info" },
                { "slug": "u-warning", "color": "rgb(251 146 60)", "name": "Udemy Warning" },
                { "slug": "u-danger", "color": "rgb(236 72 153)", "name": "Udemy Danger" }

            ],
            "defaultGradients": false,
            "background": true,
            "text": true,
            "link": true,
            "custom": true,
            "duotone": [
                {"slug": "u-pink-sunset","colors": ["#11245e","#dc4379"], "name": "Udemy pick Sunset"},
                {"slug": "u-green-sunset" ,"colors": ["#4fc149" , "#d2db23"] , "name": "Udemy Green"}
            ],
            "gradients": [{"slug": "udemy-gradeing-demo", "name": "Udemy gradient", "gradient": "linear-gradient(#a8ff78, #78ffd6)"}]
            
        },
        "blocks" : {
            "core/preformatted": {
                "typography": {
                    "fontSizes": [],
                    "customFontSize": false
                }
            },
            "core/site-title" : {
                "typography": {
                    "fontFamilies": [{"name": "Udemy Pacifico", 
                    "slug": "u-font","fontFamily": "Pacifico"}]
                },
                "color" : {
                    "defaultPalette":  true,
                    "palette": [
                        {"slug": "u-p-primiary" , "color": "rgb(50 168 68)" , "name": "Sandeep Color"}
                    ],
                    "link": false,
                    "custom": true,
                    "gradients": [],
                    "defaultGradients": false
                }
            },
            "core/cover": {
                "color": {
                    "duotone": [],
                    "customDuotone": false 
                }
            }
            
        }
    }

    
}